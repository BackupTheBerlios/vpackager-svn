PUBLIC SUB Form_Open()

  ME.Caption = "About " & GLobal.sMyname
  ME.tlBanner.text = "<h1>" & Global.sMyname & " " & Global.sMyVer & "</h1>"

END

PUBLIC SUB btClose_Click()

  ME.Close()
  FMain.Show
  

END

PUBLIC SUB Form_Close()

  Fmain.Show()
  

END
